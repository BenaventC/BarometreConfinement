	file = {Kobayashi et al. - 2018 - Text Mining in Organizational Research.pdf:C\:\\Users\\chris\\Zotero\\storage\\3NLIDHUY\\Kobayashi et al. - 2018 - Text Mining in Organizational Research.pdf:application/pdf}
